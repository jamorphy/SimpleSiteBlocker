{
  "manifest_version": 2,
  "name": "Site Blocker",
  "version": "1.0",
  "description": "Block websites from a customizable list",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
